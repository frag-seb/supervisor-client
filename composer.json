{
  "name": "frag-seb/supervisor-client",
  "description": "The php client for supervisor",
  "keywords": ["supervisor","api","client"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jochen Mandl",
      "email": "mandl.js@googlemail.com"
    }
  ],

  "autoload": {
    "psr-4": {
      "FragSeb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FragSeb\\Supervisor\\Test\\": "tests/Supervisor/case/"
    }
  },
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "mockery/mockery": "^0.9.8",
    "phpspec/phpspec": "^3.0",
    "satooshi/php-coveralls": "dev-master",
    "leanphp/phpspec-code-coverage": "^3.2@dev"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
